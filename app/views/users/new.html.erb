<h1>회원가입</h1>
<p>Find me in app/views/users/new.html.erb</p>
<%= flash[:alert] %>
<%= form_for @user do |f| %>
    Email: <%= f.email_field :email, value: params[:email] %><br>
    Nickname: <%= f.text_field :nickname, value: params[:nickname] %><br>
    Password: <%= f.password_field :password %><br>
    Password Confirmation: <%= f.password_field :password_confirmation%><br>
    <%= f.submit '회원가입하기' %>
<% end %>